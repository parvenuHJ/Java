import java.util.Scanner;

public class Q17_정수n을입력받아 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		System.out.print("n 입력 : ");
		int n = sc.nextInt();
		int start = 1;
		
		for ( int i = 0 ; i < n ; i++) {
			start = start + i;
			System.out.print(start + " ");
		}
		
		
		
		
		
		
		if ( n == 1 ) {
			n=n+0; // n=1+0 = 1
		}else if (n ==2 ) {
			n = n+1; // n=1+1 = 2
		}else if (n ==3 ) {
			n = n+2; // n = 2+2 = 4
		}else if (n ==4) {
			n = n+3; // n = 4+3 = 7
			
		}
		
		
		
		
	
	}

}
